<configuration>
    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%msg%n</pattern>
        </encoder>
    </appender>

    <logger name="org.apache.poi" level="WARN"/>

    <root level="${root-level:INFO}">
        <appender-ref ref="CONSOLE"/>
    </root>
</configuration>